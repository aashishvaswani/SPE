pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git branch: 'main', 
                    credentialsId: 'github-ssh',  // Use correct Jenkins credentials ID
                    url: 'git@github.com:aashishvaswani/SPE.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x Prog1.py"
                sh "./Prog1.py"
            }
        }
        stage('Test Code') {
            steps {
                sh "chmod u+x Test2.py"
                sh "./Test2.py"  // Fixed the filename (was "Test.py" but should be "Test2.py" for consistency)
            }
        }
    } 
}
