pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'git@github.com:aashishvaswani/SPE.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x Prog1.py"
                sh "./Prog1.py"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x Test2.py"
                sh "./Test.py"
            }
        }
    } 
}